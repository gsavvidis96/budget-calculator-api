{
  "name": "test",
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "start": "rimraf dist && concurrently \"npx tsc --watch\" \"nodemon --exec serverless offline --stage development\"",
    "migrations:generate": "drizzle-kit generate:pg",
    "migrate": "dotenv -e .env.development -- ts-node ./src/db/migrate.ts",
    "login:development": "dotenv -e .env.development -- ts-node ./src/test/userLogin.ts",
    "test": "vitest"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.6.1",
    "drizzle-orm": "^0.29.3",
    "firebase-admin": "^12.0.0",
    "ws": "^8.16.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.131",
    "@types/node": "18.17.1",
    "@types/ws": "^8.5.10",
    "concurrently": "^8.2.2",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "^0.20.10",
    "firebase": "^10.7.1",
    "nodemon": "^3.0.2",
    "postgres": "^3.4.3",
    "rimraf": "^5.0.5",
    "serverless": "^3.38.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-offline": "^13.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  }
}
